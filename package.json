{
  "name": "diff-action",
  "version": "0.1.0",
  "private": true,
  "description": "GitHub Action that compares two files, add tolerances, send notifications, etc",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint '**/*.js' '**/*.ts'",
    "pack": "ncc build -m",
    "format": "prettier --write '**/*.js' '**/*.ts'",
    "all": "npm run build && npm run lint && npm run pack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anchan828/diff-action.git"
  },
  "keywords": [
    "github",
    "actions",
    "diff"
  ],
  "author": "Louis Brunner",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.6.0",
    "@actions/github": "5.0.0",
    "diff": "5.0.0"
  },
  "devDependencies": {
    "@types/diff": "5.0.2",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.26",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "@zeit/ncc": "0.21.1",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jest": "26.1.3",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "27.5.1",
    "jest-circus": "27.5.1",
    "prettier": "2.6.1",
    "ts-jest": "27.1.4",
    "typescript": "4.6.3"
  }
}
